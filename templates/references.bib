% BibTeX References for [TOPIC_NAME]
%
% Citation Key Format: AuthorYearKeyword (e.g., Smith2023AI)
% Keep keys short, unique, and descriptive
%
% Entry Types:
% - @article: Journal articles
% - @inproceedings: Conference papers
% - @book: Books
% - @techreport: Technical reports
% - @misc: Websites, blog posts, other
%
% Required fields vary by type - see examples below

% ============================================================
% JOURNAL ARTICLES
% ============================================================

@article{ExampleAuthor2023,
    author = {Last, First and Last, First},
    title = {Example Article Title},
    journal = {Journal Name},
    year = {2023},
    volume = {10},
    number = {5},
    pages = {123--145},
    doi = {10.1234/example},
    url = {https://example.com/paper},
    note = {[Brief note about relevance]}
}

% ============================================================
% CONFERENCE PAPERS
% ============================================================

@inproceedings{ExampleConference2023,
    author = {Last, First and Last, First},
    title = {Example Conference Paper Title},
    booktitle = {Proceedings of the Conference Name},
    year = {2023},
    pages = {100--110},
    doi = {10.1234/example},
    url = {https://example.com/paper},
    note = {[Brief note]}
}

% ============================================================
% BOOKS
% ============================================================

@book{ExampleBook2022,
    author = {Last, First},
    title = {Example Book Title},
    publisher = {Publisher Name},
    year = {2022},
    edition = {2nd},
    isbn = {978-1-234567-89-0},
    url = {https://example.com/book},
    note = {[Brief note]}
}

% ============================================================
% TECHNICAL REPORTS
% ============================================================

@techreport{ExampleReport2023,
    author = {Organization Name},
    title = {Example Technical Report Title},
    institution = {Institution Name},
    year = {2023},
    number = {Report-2023-01},
    url = {https://example.com/report},
    note = {[Brief note]}
}

% ============================================================
% WEBSITES & ONLINE SOURCES
% ============================================================

@misc{ExampleWebsite2024,
    author = {Author or Organization},
    title = {Example Webpage Title},
    year = {2024},
    url = {https://example.com/page},
    note = {Accessed: 2024-01-15. [Brief note about credibility]}
}

